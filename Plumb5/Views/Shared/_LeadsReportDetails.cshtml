<div class="popupcontainer hideDiv" id="div_EditFollowupNote">
    <div class="popupItem w-450 m-p-wid-300 position-relative">
        <div class="popuptitle">
            <div class="popuptitlwrp">
                <h6 id="mainTab">Manage Lead</h6>
                <small class="popupsubtitle"></small>
            </div>
            <i id="close-popup" class="icon ion-close"></i>
        </div>
        <div class="popupbody">
            <div class="popupbodycont">

                <div id="lmseditlead" class="lmspopupswindwrp">
                    <div class="lmsedittabwrap">
                        <div data-lmsedittab="editleadtabscontent" class="lmsedittabitem active" id="tabLead">Edit Leads</div>
                        <div data-lmsedittab="lmsaddfollowup" class="lmsedittabitem" id="tabFollowup">Add Follow-Up</div>
                        <div data-lmsedittab="lmsaddnotes" class="lmsedittabitem" id="tabNote">Add Notes</div>
                    </div>
                    <div class="lmstabcontentwrap pad-20">
                        <div id="editleadtabscontent" class="editlmstabscont">

                            @{
                                Html.RenderPartial("_CreateLead");
                            }

                        </div>
                        <div id="lmsaddfollowup" class="editlmstabscont hideDiv">
                            @{
                                Html.RenderPartial("_AddFollowUp");
                            }

                        </div>
                        <div id="lmsaddnotesNew" class="editlmstabscont hideDiv">
                            @{
                                Html.RenderPartial("_AddLmsNotes");
                            }
                        </div>
                    </div>

                </div>

            </div>
        </div>

    </div>

</div>

