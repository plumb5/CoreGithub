<div class="row">
    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
        <div class="box-white border-top-0">
            <div class="tableWrapper" style="margin-bottom: 15px;">
                <div class="table-responsive sticky-hscroll">
                    <table class="table manageContacts" id="ui_tblReportData">
                        <thead id="ui_trheadReportData">
                            <tr>
                                <th id="dv_CheckBox" class="td-wid-2 m-p-w-40" scope="col">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input selchbxall" id="contacts_check" name="example1">
                                        <label class="custom-control-label" for="contacts_check"></label>
                                    </div>
                                </th>
                                <th scope="col" class="helpIcon m-p-w-120 td-wid-8">
                                    <div class="sortWrap">
                                        <i class="icon ion-arrow-down-c addColor"></i>
                                    </div>
                                    Label<i class="icon ion-ios-help-outline"></i>
                                    <div class="toottipWrap"></div>
                                </th>
                                <th scope="col" class="helpIcon m-p-w-220 td-wid-19">
                                    <div class="sortWrap">
                                        <i class="icon ion-arrow-down-c addColor"></i>
                                    </div>
                                    Name<i class="icon ion-ios-help-outline"></i>
                                    <div class="toottipWrap"></div>
                                </th>
                                <th class="helpIcon m-p-w-190 td-wid-19" scope="col">
                                    <div class="sortWrap">
                                        <i class="icon ion-arrow-down-c addColor"></i>
                                    </div>
                                    Email<i class="icon ion-ios-help-outline"></i>
                                    <div class="toottipWrap"></div>
                                </th>
                                <th class="helpIcon m-p-w-190 td-wid-19" scope="col">
                                    <div class="sortWrap">
                                        <i class="icon ion-arrow-down-c addColor"></i>
                                    </div>
                                    Phone Number<i class="icon ion-ios-help-outline"></i>
                                    <div class="toottipWrap"></div>
                                </th>
                                <th class="helpIcon m-p-w-160 td-wid-13" scope="col">
                                    Stage
                                    <div class="toottipWrap"></div>
                                </th>
                                <th class="helpIcon m-p-w-190 td-wid-20" scope="col">
                                    <div class="sortWrap">
                                        <i class="icon ion-arrow-down-c addColor"></i>
                                    </div>
                                    Last Updated Date
                                    <div class="toottipWrap"></div>
                                </th>
                            </tr>
                        </thead>
                        <tbody id="ui_tbodyReportData"></tbody>
                    </table>
                </div>
            </div>
            @{
                Html.RenderPartial("_Paging");
            }
        </div>
    </div>
</div>



