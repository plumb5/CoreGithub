
<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Contact Imports</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javasctipt:void(0);">Contacts</a></li>
            <li class="breadcrumb-item active" aria-current="page">Contact Imports</li>
        </ol>
    </div>
    <div class="box-white mr-tp-15">
        <div class="contimportrepwrap">
            <div class="showDateIconWrap pt-2 pb-2 border-bottom">
                @{
                    Html.RenderPartial("_PageHeadingWithDate");
                }
                <div class="btngroupwrap d-flex align-items-center pr-2">
                    <div class="downloadsampwrap">
                        <button type="button" class="btn btn-second btn-sm ContributePermission" title="Download Sample file" id="ui_btnContactSampleDownLoad">
                            <i class="icon ion-android-download m-auto"></i>
                            <span>Download Sample file</span>
                        </button>
                    </div>
                    <div class="startbtnwrap ml-2 hideDiv" id="ui_divStartImport">
                        <div class="btn-group">
                            <button type="button" class="btn btn-primary ContributePermission">
                                Start an Import
                            </button>
                            <button type="button" class="btn btn-primary dropdown-toggle dropdown-toggle-split"
                                    data-toggle="dropdown" aria-expanded="false">
                                <span class="sr-only">Toggle Dropdown</span>
                            </button>
                            <div class="dropdown-menu">
                                <a id="ui_aStartImport" class="dropdown-item" href="javascript:void(0);">via File</a>
                                <a id="ui_aviaftpbtn" class="dropdown-item" href="javascript:void(0);">via FTP</a>
                                <a id="ui_aviaapibtn" class="dropdown-item" href="javascript:void(0);">via API</a>
                                <a id="ui_aviagooglesheetbtn" class="dropdown-item" href="javascript:void(0);">via Google Sheet</a>
                                <a id="ui_aviagoogleadwordsbtn" class="dropdown-item" href="javascript:void(0);">via Google Adwords</a>
                            </div>
                        </div>
                    </div>&ensp;
                    @{
                        Html.RenderPartial("_Export");
                    }
                </div>
            </div>
            <div class="importTableWrap">
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <div class="box-white border-0 position-relative">
                            <div class="tableWrapper">
                                <div class="table-responsive contimportrep">
                                    <table id="ui_tblReportData" class="table">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="helpIcon m-p-w-190 td-wid-20">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>
                                                    File Name
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-8" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>
                                                    Total
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-160 td-wid-11" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>
                                                    Completed
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-140  td-wid-10" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>
                                                    Success
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-140 td-wid-9" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>
                                                    Merge
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-10" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>
                                                    </div>
                                                    Rejected
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-170 td-wid-12" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>
                                                    </div>
                                                    Updated Date
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-10" scope="col">
                                                    Progress
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ui_tbodyReportData"></tbody>
                                    </table>
                                </div>
                            </div>
                            @{
                                Html.RenderPartial("_Paging");
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
