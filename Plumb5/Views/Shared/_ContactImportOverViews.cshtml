<!--Popup window right-->
<div class="popupcontainer hideDiv" id="dvFTPcontact">
    <div class="popupItem w-450 m-p-wid-300 position-relative">
        <div class="popuptitle">
            <h6>Add Stage</h6>
            <i id="close-popup" class="icon ion-close"></i>
        </div>
        <div class="popupbody position-relative">
            <div class="popupbodycont pad-20">
                <div class="addftpconnect">
                    <div class="noftpcontwrp hideDiv">
                        <p class="font-13 text-color">
                            No FTP Connection available. Please Add an FTP connection in <a href="contacts-settings.html">
                                Contact
                                Settings
                            </a>
                        </p>
                    </div>
                    <div class="form-group">
                        <label class="frmlbltxt" for="">Connection Name</label>
                        <select name="" class="form-control" id="ui_connectionnames">
                            <option value="0">Select</option>
                        </select>
                    </div>
                    <div class="btnwrp df-ac-sbet">
                        <div class="ftprootwrp w-300">
                            <div class="inptftpwrp hideDiv">
                                <input type="text" class="form-control inptftp" title="" name=""
                                       value="" id="ftpeditfldinpt">
                                <i id="ftprooteditfld" class="fa fa-pencil editftproot"></i>
                                <i id="ftprootclsefld" class="fa fa-close clseftproot hideDiv"></i>
                            </div>
                        </div>
                        <button id="ftpconnectbtn" type="button" class="btn btn-primary">Connect</button>
                    </div>
                    <div id="ftpfilecontactlist" class="box-white mt-3 p-3 hideDiv">
                        <div class="form-group position-relative">
                            <input type="text" name="" class="form-control" placeholder="Search Files Here" id="protocolnum">
                            <i class="fa fa-search filesearchicn"></i>
                        </div>
                        <div class="ftptblwrp bg-white">
                            <div class="table-responsive min-h-100 border border-bottom-0">
                                <table class="table mb-0">
                                    <thead>
                                        <tr>
                                            <th class="td-wid-10 m-p-w-40" scope="col">
                                                <div class="custom-control custom-checkbox">
                                                    <input type="checkbox" class="custom-control-input lmsleadpropAll" id="configfeatall"
                                                           name="example1">
                                                    <label class="custom-control-label" for="configfeatall"></label>
                                                </div>
                                            </th>
                                            <th class="td-wid-60 m-p-w-170" scope="col">File Name</th>
                                            <th class="td-wid-30 m-p-w-170 hideDiv" scope="col">No. of Rows</th>
                                        </tr>
                                    </thead>
                                    <tbody id="ui_filesdata">
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="ftpdrpdwn mt-4">
                            <div class="form-group">
                                <label class="frmlbltxt" for="">Group Name</label>
                                <select class="form-control addgroupNamedrop" name="" id="ui_addgroupdetails" data-placeholder="Add Fields">
                                    <option value="0">Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="startcheckbx mt-4">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" id="existContacts">
                                <label class="custom-control-label font-12" for="existContacts">
                                    Do not add existing contacts to
                                    this group <i class="icon ion-help-circled strtimpinfo" data-toggle="popover"
                                                  data-content="if the contact is part of the any other group(s)in the system. Contacts present in the import file will not be added to this group."></i>
                                </label>
                            </div>
                            <div class="custom-control custom-checkbox mt-2">
                                <input type="checkbox" class="custom-control-input" id="removeContacts">
                                <label class="custom-control-label font-12" for="removeContacts">
                                    Empty selected group before
                                    importing. <i class="icon ion-help-circled strtimpinfo" data-toggle="popover"
                                                  data-content="Clear all existing contacts present in this group prior to importing."></i>
                                </label>
                            </div>
                            <div class="custom-control custom-checkbox mt-2">
                                <input type="checkbox" class="custom-control-input" id="retainassignconts">
                                <label class="custom-control-label font-12" for="retainassignconts">
                                    Override Assignments <i class="icon ion-help-circled strtimpinfo" data-toggle="popover"
                                                            data-content="All existing Leads that are present in the system will be assigned to a new agent if present in the import file, else it will remain unchanged."></i>
                                </label>
                            </div>
                            <div class="custom-control custom-checkbox mt-2">
                                <input type="checkbox" class="custom-control-input" id="ui_chkAssignUser">
                                <label class="custom-control-label font-12" for="ui_chkAssignUser">
                                    Assign Individually <i class="icon ion-help-circled strtimpinfo" data-toggle="popover"
                                                           data-content="Selects Agents to be assigned in a round-Robin manner for the imported Leads."></i>
                                </label>
                            </div>
                            <div class="custom-control custom-checkbox mt-2">
                                <input type="checkbox" class="custom-control-input" id="overridesources" name="assingindivconts">
                                <label class="custom-control-label font-12" for="overridesources">
                                    Override Sources <i class="icon ion-help-circled strtimpinfo" data-toggle="popover"
                                                        data-content="All existing Leads that are present in the system will be assigned to a new Source if present in the import file, else it will remain unchanged."></i>
                                </label>
                            </div>
                        </div>
                        <div class="ftpterms mt-4">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" checked="" id="customCheck1">
                                <label class="custom-control-label font-12" for="customCheck1">
                                    I agree that all contacts in this
                                    import are expecting to hear from my organization. I have a prior relationship with these contacts
                                    and have emailed them at least once in the past year.
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="spinnerftpwrp hideDiv">
                <div class="sk-wave">
                    <div class="sk-rect sk-rect1"></div>
                    <div class="sk-rect sk-rect2"></div>
                    <div class="sk-rect sk-rect3"></div>
                    <div class="sk-rect sk-rect4"></div>
                    <div class="sk-rect sk-rect5"></div>
                </div>
            </div>
        </div>
        <div class="popupfooter">
            <div class="btnwrp">
                <button id="startImprtbtn" class="btn btn-primary mr-2" type="button">Start Import</button>
                <button class="btn btn-secondary clsepopup" type="button">Cancel</button>
            </div>
        </div>
    </div>
</div>

