@{
    ViewBag.Title = "Plumb5 :: In App Responses";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<link href="~/Content/select2/css/select2.min.css" rel="stylesheet" />
<script src="~/Content/select2/js/select2.full.min.js"></script>

<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Reports</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0);">Mobile</a></li>
            <li class="breadcrumb-item"><a href="javascript:void(0);">In-App Notifications</a></li>
            <li class="breadcrumb-item active" aria-current="page">Reports</li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem">
                <a class="tap-nav-link active" href="javascript:void(0);"> In-App Responses </a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/MobileInApp/Reports/FormResponses"> Form Responses </a>
            </div>
        </div>
        <div class="iconsWrap">
            @{ Html.RenderPartial("_Export"); }
        </div>
    </div>
    <div class="topTabWrap brd-TRL">
        @{ Html.RenderPartial("_PageHeadingWithDate"); }
        <div class="iconsWrap-docu">
            <div class="emailtempdropdown w-200 mr-2 m-w-150px">
                <select class="form-control addCampName" data-placeholder="Search Form" id="ui_drpdwn_MobileForms">
                    <option value="NULL">Search Form</option>
                </select>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white">
                <div class="tableWrapper">
                    <div class="table-responsive min-h-400">
                        <table id="ui_tblReportData" class="table tbl-frmrepresp">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-220 td-wid-20 text-left">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Form Name
                                    </th>
                                    <th class="helpIcon m-p-w-190 td-wid-15" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Type
                                    </th>
                                    <th class="helpIcon m-p-w-150 td-wid-15" scope="col">
                                        Impressions
                                    </th>
                                    <th class="helpIcon m-p-w-150 td-wid-15" scope="col">Responses</th>
                                    <th class="helpIcon m-p-w-150 td-wid-15" scope="col">Closed</th>
                                    <th class="helpIcon m-p-w-150 td-wid-15" scope="col">
                                        Conversion Rate
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_tbodyReportData"></tbody>
                        </table>
                    </div>
                </div>
                @{ Html.RenderPartial("_Paging"); }
            </div>
        </div>
    </div>
</section>

<script src="~/js/MobileInApp/InAppResponses.js"></script>