@{
    ViewBag.Title = "WebPushIndividualResponse";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Alert and Notification</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Web</a></li>
            <li class="breadcrumb-item">
                <a href="#">Browser Notification</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">
                Alert and Notification
            </li>
        </ol>
    </div>
    <div class="box-white mr-tp-15">
        <div class="showDateIconWrap">
            <div class="showDateWrap brd-bttm-none mb-0">
                @{
                    Html.RenderPartial("_PageHeadingWithDate");
                }
            </div>
            <div class="searchWrap">
                <div class="searchInput">
                    <input class="form-control form-control-sm contsearchfocus" type="text" name="" placeholder="Search by machineid" id="txt_SearchBy">
                    <div id="ui_div_SearchIcon" class="searchIcon">
                        <i class="icon ion-android-search"></i>
                    </div>
                </div>
                @{
                    Html.RenderPartial("_Export");
                }
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white border-top-0 position-relative">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table id="ui_tblReportData" class="table sms-tbl popuppreveye-tbl">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-80 td-wid-5 text-center">Preview</th>

                                    <th scope="col" class="helpIcon m-p-w-190 td-wid-40">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>
                                        Template Name
                                        <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-150 td-wid-30" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>
                                        Sent Date<i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-190 td-wid-30">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>
                                        Subscriber
                                        <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_tbodyReportData">
                            </tbody>
                        </table>
                    </div>
                </div>
                @{
                    Html.RenderPartial("_Paging");
                }
            </div>
        </div>
    </div>
</section>
<div class="bnotiftempwrp hideDiv" id="TemplatePreview">
    <div class="notifprevmain">
        <div class="notifbanwrp"></div>
        <div class="notiprevcontent">
            <div class="notiprevcontwrp">
                <div class="notifbelcont">
                    <div class="notificonwrp"></div>
                    <div class="notifititcontwrp">
                        <h3 class="notiftitle">Title</h3>
                        <p class="notifdescript">Message</p>
                    </div>
                </div>
                <div class="notifbtnwrp mt-2">
                    <button id="notifsubbtn" type="button" class="btn btn-notifprimary w-49 mr-1">
                        <a class="bnotbtn1" href="#">Take a Look</a>
                    </button>
                    <button id="notifcanbtn" type="button" class="btn btn-notisecondary w-49">
                        <a class="bnotbtn2"
                           href="#">Cancel</a>
                    </button>
                </div>

            </div>
        </div>
    </div>
</div>
<script src="~/js/WebPush/WebPushIndividualResponse.js"></script>