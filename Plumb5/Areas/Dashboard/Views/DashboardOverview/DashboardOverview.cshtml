<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Dashboard</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item active" aria-current="page">Dashboard</li>
        </ol>
    </div>
    <div class="box-white mr-tp-15">

        <div class="showDateIconWrap">
            <div class="showDateWrap brd-bttm-none mb-0">
                @{
                    Html.RenderPartial("_PageHeadingWithDate");
                }
            </div>


            <div class="iconsWrap">
                <div class="dropdown">
                    <i class="icon ion-ios-calendar-outline" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></i>
                    <div id="dvDashBoardAlert" class="dropdown-menu dropdown-menu-right keepopen" aria-labelledby="addLandingpage" x-placement="bottom-end" style="position: absolute; transform: translate3d(18px, 32px, 0px); top: 0px; left: 0px; will-change: transform;">
                        <div class="drop-form-dash p-4">
                            <h4 class="dropdown-head">Send Report Via Mail</h4>
                            <div class="sendmailoptionwrp mb-3">
                                <div class="custom-control custom-radio custom-control-inline">
                                    <input checked type="radio" id="senddashboardrep" name="sendreporttype" class="custom-control-input" value="senddashboardreports">
                                    <label class="custom-control-label font-weight-bold" for="senddashboardrep">Send Analytical Report</label>
                                </div>
                                <div class="custom-control custom-radio custom-control-inline">
                                    <input type="radio" id="sendleadrep" name="sendreporttype" class="custom-control-input" value="sendleadreports">
                                    <label class="custom-control-label font-weight-bold" for="sendleadrep">Send Leads Report</label>
                                </div>
                            </div>
                            <div id="senddashboardreports" class="dashboardreprtmail">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="TO" id="txtToEmailId" />
                                    <small class="text-muted font-11">
                                        Add multiple emails ids followed by
                                        commas.
                                    </small>
                                </div>
                                <div class="checkeebx-group row w-100 mb-3 overflow-x-vis">
                                    <div class="lblwrap col-sm-4 col-md-4 col-lg-4 col-xl-4 text-right-lbl">
                                        <label class="frmlbltxt" for="">Frequency</label>
                                    </div>
                                    <div class="col-sm-8 col-md-8 col-lg-8 col-xl-8">
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="Weekly" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="Weekly">Weekly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">Weekly Report</div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the
                                                            user on every 8th, 16th, 22nd &amp; 29th of Every
                                                            Month. The report shows metrics for 7 days from
                                                            the received Date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="Fornightly" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="Fornightly">Fortnightly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">Fornightly Report</div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the
                                                            user on every 16th &amp; 29th of Every Month. The
                                                            report shows metrics for 15 days from the
                                                            received Date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="Monthly" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="Monthly">Monthly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">Monthly Report</div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the
                                                            user on the 1st of Every Month. The report shows
                                                            metrics for past month from the received Date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="Quarterly" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="Quarterly">Quarterly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">Quarterly Report</div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the
                                                            user on the 1st of Every Month. The report shows
                                                            metrics for past 3 months from the received
                                                            Date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                    </div>

                                </div>
                                <div class="btnWrap">
                                    <button type="button" class="btn-active btn-block" id="ui_btnMailAlert" data_id="0">Save</button>
                                </div>
                            </div>
                            <div id="sendleadreports" class="dashboardreprtmail hideDiv">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="TO" id="txtToEmailIdLead">
                                    <small class="text-muted font-11">
                                        Add multiple emails ids followed by
                                        commas.
                                    </small>
                                </div>
                                <div class="checkeebx-group row w-100 mb-3 overflow-x-vis">
                                    <div class="lblwrap col-sm-4 col-md-4 col-lg-4 col-xl-4 text-right-lbl">
                                        <label class="frmlbltxt" for="">Frequency</label>
                                    </div>
                                    <div class="col-sm-8 col-md-8 col-lg-8 col-xl-8">
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="DailyLead" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="DailyLead">Daily</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">
                                                            Daily Report
                                                        </div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the user on every day.
                                                            The report shows metrics for 24 hours.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="WeeklyLead" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="WeeklyLead">Weekly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">
                                                            Weekly Report
                                                        </div>
                                                        <div class="dashtooltipbody">
                                                            Report mailed to the user on Every
                                                            Monday before EOD. The report shows
                                                            metrics for 7 days from the received
                                                            date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" id="MonthlyLead" name="mailfrequency" class="custom-control-input">
                                            <label class="custom-control-label" for="MonthlyLead">Monthly</label>
                                            <span>
                                                <i class="icon ion-information-circled dashinfotip">
                                                    <div class="dashtooltipwrp">
                                                        <div class="dashtooltiptit">
                                                            Monthly Report
                                                        </div>
                                                        <div class="dashtooltipbody">
                                                            Report Mailed to the user on the 1st of
                                                            Every Month. The report shows metrics
                                                            for past month from the received Date.
                                                        </div>
                                                    </div>
                                                </i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="btnWrap">
                                    <button type="button" class="btn-active btn-block" id="ui_btnMailAlertLead" data_id="0">
                                        Save
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <i class="icon ion-grid font-22 gridsave" title="Arrange"></i>
                <i class="fa fa-floppy-o font-20 saveIcnbtn hideDiv" title="Save"></i>
                <i class="icon ion-ios-gear-outline icndisble widgticnadd" title="Edit Dashboard"></i>
            </div>
        </div>
    </div>
</section>
<section class="sheet">
    <div class="widgetswrp mt-4">
        <div class="grid-stack">
        </div>
    </div>
    <div class="nodatadashwrp mt-4 hideDiv">
        <div class="box-white h-100 df-ac-cc">
            <p>
                Click on the <i class="icon ion-android-settings"></i> setting icon <br> to add widgets
                for your dashboard.
            </p>
        </div>
    </div>
</section>



