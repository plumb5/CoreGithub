@{
    ViewBag.Title = "Plumb5 :: Dashboard";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<link rel="stylesheet" href="~/Content/gridstack.min.css" />
<script src="~/js/gridstack.all.js"></script>
<script src="//code.jquery.com/jquery-1.10.2.js"></script>
