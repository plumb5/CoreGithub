    ViewBag.Title = "Plumb5 :: Agent Notification";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}
<link href="~/Content/dist/devices.css" rel="stylesheet" />
<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Settings</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0);">Lead Management</a></li>
            <li class="breadcrumb-item active" aria-current="page">Settings</li>
        </ol>
    </div>
    <div class="setttabwrap mr-tp-15">
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/Settings">Stages</a></div>
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/InactiveNotification">In-Activity Notification</a></div>
        @*<div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/NotificationRules">Notification Rules</a></div>*@
        <div class="setttabitem active" data-setttype="javascript:void(0);"><a href="javascript:void(0);">Lead Notification</a></div>
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/Settings/AlertNotification">Alert Notification</a></div>
        @*<div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/LeadAssignNotificationSetting">Lead Assignment Agent Notification</a></div>*@
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/CallSettings">Call Settings</a></div>
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/LeadProperties">Lead Properties</a></div>
        <div class="setttabitem" data-setttype="javascript:void(0);"><a href="/Prospect/AdvancedSettings/">Advanced Settings</a></div>

    </div>
    <div class="settcontainer">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <div class="box-white">
                    <div id="inactivitynotif" class="setttabcontainer">
                        <div class="row">
                            <div class="col-sm-12 col-md-4 col-xl-4 col-lg-4">
                                <div class="lmsinactivnotifwrp p-3">
                                    <div class="lmsassignuserchkbxwrp mt-3">
                                        <label for="" class="frmlbltxt w-100">
                                            Auto Alert to Lead On Sales Assignment
                                        </label>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" class="custom-control-input" id="ui_checkautoNotificationMail" name="AutoNotificationMail" value="1">
                                            <label class="custom-control-label" for="ui_checkautoNotificationMail">Mail</label>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" class="custom-control-input" id="ui_checkautoNotificationSms" name="AutoNotificationSms" value="0">
                                            <label class="custom-control-label" for="ui_checkautoNotificationSms">SMS</label>
                                        </div>
                                        <div class="custom-control custom-checkbox custom-control-inline">
                                            <input type="checkbox" class="custom-control-input" id="ui_checkautoNotificationWA" name="AutoNotificationWA" value="2">
                                            <label class="custom-control-label" for="ui_checkautoNotificationWA">WhatsApp</label>
                                        </div>
                                    </div>
                                    <div class="lmssavebtnwrp mt-4">
                                        <button id="ui_btnSaveAutoNotification" type="button" class="btn btn-primary">Save</button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-8 col-xl-8 col-lg-8">
                                <div class="leadnotiftempwrap">
                                    <div class="leadnotiftabwrap">
                                        <div class="btn-group">
                                            <button data-tabcontname="notifmailbody" type="button" class="btn btn-light leadnotiftabname active"><i class="fa fa-eye"></i> Mail</button>
                                            <button data-tabcontname="notifsmsbody" type="button" class="btn btn-light leadnotiftabname"><i class="fa fa-eye"></i> SMS</button>
                                            <button data-tabcontname="notifwhatsappbody" type="button" class="btn btn-light leadnotiftabname"><i class="fa fa-eye"></i> WhatsApp</button>
                                        </div>
                                    </div>
                                    <div class="leadnofitabcontwrap">
                                        <div id="notifmailbody" class="leadnofittabcont">
                                            <div class="tabcontprevwrap">
                                                <div class="mailDeskWrap">
                                                    <div class="mailbodyWrap">
                                                        <iframe src="https://plumb5.com/LeadNotification/ThankYouAlertToLeads.html"
                                                                frameborder="0"></iframe>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="notifsmsbody" class="leadnofittabcont hideDiv">
                                            <div class="tabcontprevwrap">
                                                <div class="androidwrap">
                                                    <div class="deviceWrap">
                                                        <div class="device device-google-pixel-2-xl mr-auto">
                                                            <div class="device-frame">
                                                                <div class="device-content">
                                                                    <div class="devicesreenwrap border-radius-5">
