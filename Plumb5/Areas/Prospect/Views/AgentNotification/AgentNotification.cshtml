                                                                            </div>
                                                                            <div class="messheadwrp">
                                                                                <div class="arrwrp"><i class="icon ion-android-arrow-back"></i></div>
                                                                                <div class="icnphone">
                                                                                    <i class="fa fa-video-camera mr-4"></i>
                                                                                    <i class="fa fa-phone mr-4"></i>
                                                                                    <i class="fa fa-ellipsis-v"></i>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="devicemesscont">
                                                                            <div class="messcont">
                                                                                <p class="messdate">Monday, Jun 13, 8:02 AM</p>
                                                                                <div class="messinboxwrp">
                                                                                    <div class="namwrp">Y</div>
                                                                                    <div class="mesinbxcont">
                                                                                        Dear [{*CustomerName*}], Thank you for contacting us! Our Sales Manager [{*UserName*}] will call you shortly via [{*UserPhone*}] Best Wishes, from [{*AccountName*}] PLUMB5
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="device-stripe"></div>
                                                            <div class="device-header"></div>
                                                            <div class="device-sensors"></div>
                                                            <div class="device-btns"></div>
                                                            <div class="device-power"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="notifwhatsappbody" class="leadnofittabcont hideDiv">
                                            <div class="tabcontprevwrap">
                                                <div class="ioswrap">
                                                    <div class="deviceWrap">
                                                        <div class="device device-iphone-8 mr-auto">
                                                            <div class="device-frame">
                                                                <div class="device-content h-100 whatsapp-bg">
                                                                    <div class="devicescreenioswrp">
                                                                        <div class="iosbathead bg-light">
                                                                            <div class="iostopbtry pl-2 pr-2">
                                                                                <div class="signwrp d-flex h-20p">
                                                                                    <i class="icon ion-android-more-horizontal mr-01"></i><i class="icon ion-android-more-horizontal"></i>
                                                                                    <i class="icon ion-wifi ml-2"></i>
                                                                                </div>
                                                                                <div class="timwrp">
                                                                                    9:41 AM
                                                                                </div>
                                                                                <div class="battp d-flex">
                                                                                    <div class="timwrp mr-1">50%</div>
                                                                                    <div class="fa fa-battery-half">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="iosmeshead h-35 border-bottom pl-2 pr-2">
                                                                                <div class="messtit d-flex align-items-center">
                                                                                    <i class="icon ion-ios-arrow-left fa-2x"></i><span class="ml-2 iosmsstxt">Message</span>
                                                                                </div>
                                                                                <div class="iosmsstxt">Details</div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="devicemescontwrp p-2">
                                                                            <div class="whatsappprevwrp previewshow">
                                                                                <div class="whatsappbox-white">
                                                                                    @*<div class="whatsappimgwrp">
                                                                                            <img class="whatsappban"
                                                                                                 src="/images/analytics-whatsapp.jpg"
                                                                                                 alt="">
                                                                                        </div>*@
                                                                                    <p class="mb-0">
                                                                                        Dear [{*CustomerName*}], Thank you for contacting us! Our Sales Manager [{*UserName*}] will call you shortly via [{*UserPhone*}] Best Wishes, from [{*AccountName*}] PLUMB5
                                                                                    </p>
                                                                                </div>
                                                                                @*<div class="btnwhatsappwrp">
                                                                                        <button type="button"
                                                                                                class="btn btnwhatsapp">
                                                                                            <i class="fa fa-external-link"></i>
                                                                                            Buy
                                                                                            Now
                                                                                        </button>
                                                                                        <button type="button"
                                                                                                class="btn btnwhatsapp">
                                                                                            <i class="fa fa-phone"></i>
                                                                                            Visit
                                                                                            Products
                                                                                        </button>
                                                                                    </div>*@
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="device-stripe"></div>
                                                                <div class="device-header"></div>
                                                                <div class="device-sensors"></div>
                                                                <div class="device-btns"></div>
                                                                <div class="device-power"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
