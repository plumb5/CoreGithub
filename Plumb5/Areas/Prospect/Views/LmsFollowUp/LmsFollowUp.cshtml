@{
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
    ViewBag.Title = "Plumb5 :: Follow Up Reports";

}
<section class="sheet">
    <div class="pageheader">
        <div class="contentHead">
            <h6 class="pagetitle">My Reports</h6>
        </div>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Lead Management</a></li>
            <li class="breadcrumb-item active" aria-current="page">
                Reports
            </li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/Reports/MyReports">All Reports</a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/SourceReports">Source Reports</a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/StageReports">Stage Reports</a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link active" href="javascript:void(0);">Follow Up Reports</a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/LmsCampaignReport">Campaign Reports</a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/PublisherReport">Publisher Reports</a>
            </div>
        </div>
        <div class="iconsWrap" id="ExportDivFollowup">
            @{ Html.RenderPartial("_Export"); }
        </div>
    </div>

    <div class="topTabWrap brd-TRL">
        <div class="showDateIconWrap">
            @{ Html.RenderPartial("_PageHeadingWithDate"); }
        </div>
        <div class="lmssearchbyname">
            <div class="position-relative">
                <div class="closeserchbynamelms">
                    <i class="icon ion-close"></i>
                </div>
                <input type="text" name="" placeholder="Search by Name" class="form-control form-control-sm" id="txtlmsfollowupsearchbyname">
                <div class="srchlmswrap">
                    <i class="icon ion-android-search lmsserchicon"></i>
                </div>
            </div>
        </div>
    </div>

    <div class="lmsreportwrapper">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <div class="box-white">
                    <div class="tableWrapper">
                        <div class="table-responsive">
                            <table class="table manageContacts">
                                <thead>
                                    <tr>
                                        <th scope="col" class="helpIcon td-wid-25">
                                            Name
                                        </th>
                                        <th class="helpIcon td-wid-25" scope="col">
                                            Planned Follow Ups
                                        </th>
                                        <th class="helpIcon td-wid-25" scope="col">
                                            Missed Follow Ups
                                        </th>
                                        <th class="helpIcon td-wid-25" scope="col">
                                            Completed Follow Ups
                                        </th>
                                    </tr>
                                </thead>
                                <tbody id="ui_tbodyReportData">
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="pagiWrap">
                        @{ Html.RenderPartial("_Paging"); }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="~/js/Prospect/LmsFollowUp.js"></script>
