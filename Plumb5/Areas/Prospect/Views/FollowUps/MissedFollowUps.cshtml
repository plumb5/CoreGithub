@{
    ViewBag.Title = "Plumb5 :: Missed Follow Ups";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<link href="~/Content/select2/css/select2.min.css" rel="stylesheet" />
<script src="~/Content/select2/js/select2.full.min.js"></script>

<section class="sheet">
    <div class="pageheader">
        <div class="contentHead">
            <h6 class="pagetitle">Follow - Ups</h6>
            <small class="totalCountTitle">Missed Follow-Ups: <span id="ui_span_FollowUpsCount"></span></small>
        </div>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0);">Lead Management</a></li>
            <li class="breadcrumb-item active" aria-current="page">Missed Follow-Ups</li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/FollowUps">
                    <i class="fa fa-clock-o font-16 mr-2 text-color-blue"></i> Planned
                </a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link active" href="javascript:void(0);">
                    <i class="fa fa-eye-slash font-16 mr-2 text-color-error"></i>Missed
                </a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link" href="/Prospect/FollowUps/CompletedFollowUps">
                    <i class="fa fa-check-circle-o font-16 mr-2 text-color-success"></i>Completed
                </a>
            </div>
        </div>
        <div class="iconsWrap">
            <div class="searchCampWrap position-relative mr-3 w-200">
                <input class="form-control contsearchfocus" type="text" id="txt_searchemailphone" placeholder="Search by Email or Phone">
                <i class="icon ion-android-search serchicon" id="SearchByEmailOrPhone"></i>
            </div>
            <div class="mastfiltwrp mr-2">
                <div class="masterfiltwrp d-flex btn btn-second btn-sm" style="height: 30px; width: 110px;">
                    <div class="lmspopuslftwind" data-lmspopuswind="Master Filter" data-lmspopupswindcont="lmsmastfilter" id="btnMasterFilterFollowUp">Master Filter</div>
                    <div class="position-relative"><i id="ui_i_filterOpen" style="font-size:12px" class="fa fa-filter ml-2"></i><i style="font-size: 16px; color: #dc3545; top: -15px;" id="ui_i_filterClose" class="icon ion-ios-close clsabste hideDiv"></i></div>
                </div>
            </div>
            @{
                Html.RenderPartial("_Export");
            }
        </div>
    </div>
    <div class="topTabWrap brd-TRL">
        @{
            Html.RenderPartial("_PageHeadingWithDate");
        }
        <div class="lmsuserstagewrp">
            <div class="lmsalluserwrp m-p-hide">
                <select id="ui_drpdwn_filterbyUser" class="form-control select2drpdwnbrd" data-placeholder="Select Follow-Up User">
                    <option value="0">Select Follow-Up User</option>
                </select>
            </div>
            <div class="lmsalluserwrp">
                <select id="ui_drpdwn_filterbystages" class="form-control select2drpdwnbrd" data-placeholder="Select Stage"></select>
            </div>
        </div>
    </div>

    <div class="subdivWrap lmsfilthite border-top-0 position-relative">
        <div class="selectedItemWrap justify-content-between">
            <div class="checkBxCount">
                <span class="checkedCount">0</span> Contacts selected
                <span style="width: 35px;">&nbsp;&nbsp;</span>
                <button style="float:right" class="btn-action" type="button" id="btnFollowUpCompleted"
                        aria-haspopup="true" aria-expanded="false">
                    Complete Followup
                </button>
            </div>

        </div>

    </div>
    @{
        Html.RenderPartial("_LeadsReportDetails");
    }
</section>

<script src="~/js/Prospect/FollowUps.js"></script>
