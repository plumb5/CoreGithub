@{
    ViewBag.Title = "Plumb5 :: Api Import Request";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<style>
    dropdwntabitem .dropdwntabs, .dropdwntabitem .dropdwntabs1, .dropdwntabitem .dropdwntabs2, .dropdwntabitem .dropdwntabs3, .dropdwntabitem .dropdwntabs4, .dropdwntabitem .dropdwntabs5, .dropdwntabitem .dropdwntabs6 {
        padding: 10px;
        font-size: 13px;
        color: #495057;
        padding: 6px 10px;
        background-color: #e9ecef;
        width: 50%;
        text-align: center;
        cursor: pointer;
    }

    .userattribwrap, .userattribwrap1, .userattribwrap2, .userattribwrap3, .userattribwrap4, .userattribwrap5, .userattribwrap6 {
        padding: 10px;
    }

        .userattribwrap .dropdown-title, .userattribwrap1 .dropdown-title, .userattribwrap2 .dropdown-title, .userattribwrap3 .dropdown-title, .userattribwrap4 .dropdown-title, .userattribwrap5 .dropdown-title, .userattribwrap6 .dropdown-title {
            background-color: transparent;
        }

    .dropdwntabs.active, .dropdwntabs1.active, .dropdwntabs2.active, .dropdwntabs3.active, .dropdwntabs4.active, .dropdwntabs5.active, .dropdwntabs6.active {
        background-color: #4662d4;
        color: #ffffff;
    }

    .mailsubiconuncondi .browse-dropdown-menu {
        position: absolute;
        top: 35px;
        left: -232px;
        display: none;
        z-index: 1000;
    }

    .mailsubiconuncondi .ion-close-circled {
        color: #b51f2e;
        position: absolute;
        top: -14px;
        right: -10px;
        font-size: 18px;
    }

    .mailsubiconuncondi.zindex10 {
        z-index: 10000 !important;
    }

    .mailsubiconuncondi {
        position: absolute;
        top: 35px;
        right: 25px;
        cursor: pointer;
        color: #1b84e7;
        z-index: 1;
    }

        .mailsubiconuncondi .ion-android-person {
            position: absolute;
            right: 0;
        }

    .dropdwntabwrap {
        width: 240px;
    }

        .dropdwntabwrap .dropdwntabitem {
            display: flex;
            align-items: center;
        }
</style>
<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Settings</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javasctipt:void(0);">Contacts</a></li>
            <li class="breadcrumb-item active" aria-current="page">Settings</li>
        </ol>
    </div>
    <div class="setttabwrap mr-tp-15">
        <div class="setttabitem"><a href="/ManageContact/Settings">Contact Merge Configuration</a></div>
        <div class="setttabitem"><a href="/ManageContact/ContactProperties">Contact Properties</a></div>
        <div class="setttabitem"><a href="/ManageContact/FtpImportSettings">FTP Import Settings</a></div>
        <div class="setttabitem active"><a href="javascript:void(0);">API Import Requests</a></div>
        <div class="setttabitem"><a href="/ManageContact/ApiImportSettings">API Import Responses</a></div>
    </div>
    <div class="box-white p-0">
        <div class="showDateIconWrap pl-2 pr-2">
            <div class="actionWrap">
                <div class="createBtnWrap">
                    @{ Html.RenderPartial("_PageHeadingWithDate"); }
                </div>
            </div>
            <div class="searchWrap">
                <div class="searchInput w-200">
                    <input class="form-control form-control-sm contsearchfocus" type="text" name="" placeholder="Search by Name" id="ui_txt_RequestsSearch" propertyname="Name" displayname="Name">
                    <div id="ui_div_SearchIcon" class="searchIcon">
                        <i class="icon ion-android-search"></i>
                    </div>
                </div>
                <div class="filterWrap dropdown mr-3">
                    <i class="icon ion-android-funnel" id="filterbycontacts" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" title="Filter By"></i>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="filterbycontacts" id="customDropdownMenu">
                        <a class="dropdown-item searchColumn" propertyname="Name" displayname="Name">Name</a>
                        <a class="dropdown-item searchColumn" propertyname="IsLmsSuccess" displayname="Lms Success">Lms Success</a>
                        <a class="dropdown-item searchColumn" propertyname="IsContactSuccess" displayname="Contact Success">Contact Success</a>
                        <a class="dropdown-item searchColumn" propertyname="RequestContent" displayname="Request Content">Request Content</a>
                    </div>
                </div>

                @{ Html.RenderPartial("_Export"); }
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white border-top-0">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table id="ui_tableReport" class="table temppreveye-tbl">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-80 td-wid-10 text-center">
                                        Preview
                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-190 td-wid-15">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Name
                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-150 td-wid-10">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Contact Success

                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-150 td-wid-20">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Contact Error Message

                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-150 td-wid-10">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Lms Success

                                    </th>
                                    <th scope="col" class="helpIcon m-p-w-150 td-wid-20">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Lms Error Message
                                    </th>
                                    <th class="helpIcon m-p-w-150 td-wid-25" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>P5UniqueId
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_tbodyReportData"></tbody>
                        </table>
                    </div>
                </div>
                @{ Html.RenderPartial("_Paging"); }
            </div>
        </div>
    </div>
</section>

<div id="ui_div_ApiWebHookResponse" class="popupcontainer hideDiv">
    <div class="popupItem  m-p-wid-300 position-relative w-650">
        <div class="popuptitle">
            <div class="popuptitlwrp">
                <h6>API Request Details</h6>
                <small id="ui_subtitle_small" class="popupsubtitle"></small>
            </div>
            <i id="close-popup" class="icon ion-close"></i>
        </div>
        <div class="popupbody">
            <table id="ui_tbl_ResponseReportData" class="table no-data-records hideDiv">
                <thead>
                    <tr>
                        <th class="td-wid-30 m-p-w-170" scope="col">Response</th>
                    </tr>
                </thead>
                <tbody id="ui_tbody_ResponseReportData">
                    <tr><td colspan="1" class='border-bottom-none'><div class='no-data'>There is no data for this view</div></td></tr>
                </tbody>
            </table>
            <div id="ui_div_ResponseJson" class="hideDiv"></div>
        </div>
        <div class="popupfooter">
        </div>
    </div>
</div>

<script src="~/js/jquery.json-editor.min.js"></script>
<script src="~/js/ManageContact/ApiImportRequest.js"></script>