@{
    ViewBag.Title = "Plumb5 :: Time Spent";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}
<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Mobile Behaviour</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Mobile</a></li>
            <li class="breadcrumb-item"><a href="#">Analytics Overview</a></li>
            <li class="breadcrumb-item active" aria-current="page">Mobile Behaviour</li>
        </ol>
    </div>

    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/NewReturn">New vs Return</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/TimeOnMobile">Time on App</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/Frequency">Frequency</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/Recency">Recency</a></div>
            <div class="tabItem"><a class="tap-nav-link active" href="/MobileAnalytics/MobileApp/TimeSpend">Time Spent</a></div>
        </div>
        @{ Html.RenderPartial("_Export"); }
    </div>
    <div class="box-white mr-tp-15">
        @{ Html.RenderPartial("_PageHeadingWithDate"); }
        <div class="charteeWrap position-relative h-350">
            <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
            <canvas id="graphData" style="display: block; height: 310px; width: 933px;" width="1399" height="465" class="chartjs-render-monitor"></canvas>
        </div>
    </div>
</section>

<section id="ui_divReportSummary" class="sheet">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white mt-4">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table id="ui_tblReportData" class="table">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-150">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Time Spent <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-120" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Sessions <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Unique Visitors <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-140" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Page Views <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">                                           
                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-140" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Average Time <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">
                                        </div>
                                    </th>
                                </tr>
                                
                            </thead>
                            <tbody id="ui_tbodyReportData"></tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>

</section>
<script src="~/js/MobileAnalytics/Mobile_TimeSpend.js"></script>