@{
    ViewBag.Title = "Screen Resolution  - Analytics | Plumb5";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Mobile Screen Resolution</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Mobile </a></li>
            <li class="breadcrumb-item"><a href="#">Analytics Overview</a></li>
            <li class="breadcrumb-item active" aria-current="page">Mobile Technology</li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/OperatingSystem">Operating System</a></div>
            <div class="tabItem"><a class="tap-nav-link active" href="/MobileAnalytics/MobileApp/ScreenResolution">Screen Resolution</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/Device">Devices</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/MobileAnalytics/MobileApp/Carrier">Carrier</a></div>
        </div>
        @{ Html.RenderPartial("_Export"); }
    </div>
    <div class="topTabWrap brd-TRL">
        @{ Html.RenderPartial("_PageHeadingWithDate");}
    </div>

    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table id="ui_tblReportData" class="table first-row">
                            <thead>
                                <tr>
                                    <th class="helpIcon td-wid-25 m-p-w-140" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        <div class="spacBet">
                                            <div>Resolution</div>
                                            <div class="th-iconWrp">
                                                <i class="icon ion-ios-help-outline"></i>
                                                <div class="toottipWrap" id="ui_div_Mobile_ScreenResolution_Resolution">&nbsp;</div>
                                            </div>
                                        </div>
                                    </th>
                                    <th class="helpIcon td-wid-25 m-p-w-150" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        <div class="spacBet">
                                            <div>Screen Views</div>
                                            <div class="th-iconWrp">
                                                <i class="icon ion-ios-help-outline"></i>
                                                <div class="toottipWrap" id="ui_div_Mobile_ScreenResolution_ScreenViews">&nbsp;</div>
                                            </div>
                                        </div>
                                    </th>
                                    <th class="helpIcon td-wid-25 m-p-w-150" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        <div class="spacBet">
                                            <div>Unique Visitors</div>
                                            <div class="th-iconWrp">
                                                <i class="icon ion-ios-help-outline"></i>
                                                <div class="toottipWrap" id="ui_div_Mobile_ScreenResolution_UniqueVisitors">&nbsp;</div>
                                            </div>
                                        </div>
                                    </th>
                                    <th class="helpIcon td-wid-25 m-p-w-150" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        <div class="spacBet">
                                            <div>Average Time</div>
                                            <div class="th-iconWrp">
                                                <i class="icon ion-ios-help-outline"></i>
                                                <div class="toottipWrap" id="ui_div_Mobile_ScreenResolution_AverageTime">&nbsp;</div>
                                            </div>
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_tbodyReportData">
                            </tbody>
                        </table>
                    </div>
                </div>
                @{ Html.RenderPartial("_Paging"); }
            </div>
        </div>
    </div>

</section>
<script src="~/js/MobileAnalytics/Mobile_ScreenResolution.js"></script>