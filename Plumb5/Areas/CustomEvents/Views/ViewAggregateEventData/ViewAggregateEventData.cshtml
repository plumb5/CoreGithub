<div id="groubydiv" class="popupcontainer hideDiv">
    <div class="popupItem w-450 m-p-wid-300 position-relative">
        <div class="popuptitle bg-primary">
            <div class="popuptitlwrp">
                <h6>Manage Properties</h6>
                <small class="popupsubtitle hideDiv">Intel I3 12th Gen Processor</small>
            </div>
            <i id="close-popup" class="icon ion-close"></i>
        </div>
        <div class="popupbody">
            <div class="popupbodycont">
                <div id="addtocartlead" class="addtocartwrp hideDiv">
                    <table class="table bg-white">
                        <tbody>
                        </tbody>
                    </table>
                </div>
                <div id="aggrecatedtable" class="aggrecatedwrp">
                    <h6 class="box-title">
                        Choose the properties to make group-by and display for
                        binding the data.
                    </h6>
                    <div class="tableWrapper p-4">
                        <div class="table-responsive">
                            <table class="table tbl-td-ta-lft border bg-white">
                                <thead>
                                    <tr>
                                        <th class="td-wid-50 m-p-w-220 m-auto">Select Fields</th>
                                        <th class="td-wid-25 m-p-w-40">Group-By</th>
                                        <th class="td-wid-25 m-p-w-40">Display</th>
                                    </tr>
                                </thead>
                                <tbody id="ui_tblCustomgroupbydiv">
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="popupfooter">
            <div class="btnwrp">
                <button class="btn btn-primary mr-2" type="button" id="btnSearch">Search</button>

                <button class="btn btn-secondary clsepopup" id="btnCancel" type="button">Cancel</button>
            </div>
        </div>
    </div>

</div>

@{
    Html.RenderPartial("_CustomEventFields");
}
