
@{
    ViewData["Title"] = "AgentReport";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<link href="~/Content/select2/css/select2.min.css" rel="stylesheet" />
<script src="~/Content/select2/js/select2.full.min.js"></script>

<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Chat Reports</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0);">Live Chat</a></li>
            <li class="breadcrumb-item active" aria-current="page">Chat Agent Reports</li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem">
                <a class="tap-nav-link" href="/Chat/Responses">
                    Visitor Responses
                </a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link active" href="javascript:void(0);">
                    Agent Reports
                </a>
            </div>
        </div>
        <div class="iconsWrap">
            @{Html.RenderPartial("_Export"); }
        </div>
    </div>
    <div class="box-white mr-tp-15">
        <div class="contimportrepwrap">
            <div class="showDateIconWrap border-bottom">
                <div class="showingWrap pt-2 pb-2">
                    @{ Html.RenderPartial("_PageHeadingWithDate"); }
                </div>
                <div class="iconsWrap-docu" id="ui_divAgentName">
                    <div class="emailtempdropdown w-200 mr-2 m-w-150px">
                        <select id="ddlAgentName" class="form-control addCampName" data-placeholder="">
                            <option label="select">Search Agents</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="frmrepsubtabwrp">
                <div class="frmsubtabitem">
                    <div id="agentperform" class="chatrepsubmenu active">
                        <span>Agent</span> Performance
                    </div>
                    <div id="pingchat" class="chatrepsubmenu">Ping <span>to Chat</span></div>
                    <div id="inschats" class="chatrepsubmenu">Initiated <span>Chats</span></div>
                    <div id="complchats" class="chatrepsubmenu">Completed <span>Chats</span></div>
                </div>
            </div>
            <div class="scheduledresponses">
                <div class="subdivWrap border-0">
                    <div class="selectedItemWrap">
                        <div class="checkBxCount">
                            <span class="checkedCount">0</span> Contacts selected
                        </div>
                        <div class="addDropdownWrap">
                            <select class="form-control form-control-sm addCampName w-100" data-placeholder="Add to Group">
                                <option label="Choose one">Add to Group</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <div class="box-white border-0 position-relative">
                            <div class="tableWrapper">
                                <div class="table-responsive campaignresponse">
                                    <table id="tbl-agentperform" class="table tbl-frmrepresp">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="helpIcon m-p-w-250 td-wid-15">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>
                                                    </div>
                                                    Agent Name & Code
                                                    <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-10" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>
                                                    </div>Taken <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-160 td-wid-10" scope="col">
                                                    Served <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-140  td-wid-10" scope="col">
                                                    Completed <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-140  td-wid-8" scope="col">
                                                    InComplete <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-8" scope="col">
                                                    Missed <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-170 td-wid-8" scope="col">
                                                    Lead <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-8" scope="col">
                                                    Customer <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-11" scope="col">
                                                    Chat to lead(%) <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-12" scope="col">
                                                    Chat to Customer(%) <i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ui_tblData_agentperform"></tbody>
                                    </table>
                                    <table id="tbl-pingchat" class="table tbl-frmrepresp hideDiv">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="helpIcon m-p-w-250 td-wid-40 text-left">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>URL<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-20" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div> Auto Ping<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-160 td-wid-20" scope="col">
                                                    Response<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-150 td-wid-20" scope="col">
                                                    Ping to Chat(%)<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ui_tblData_pingchat"></tbody>
                                    </table>
                                    <table id="tbl-inschats" class="table tbl-frmrepresp hideDiv">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="helpIcon m-p-w-250 td-wid-40 text-left">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Initiated URL<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-20" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Chat Conversion<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ui_tblData_inschats"></tbody>
                                    </table>
                                    <table id="tbl-complchats" class="table tbl-frmrepresp hideDiv">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="helpIcon m-p-w-250 td-wid-25 text-left">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Date<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-15" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Taken<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-15" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Assigned<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-15" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Unassigned<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-15" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Completed<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                                <th class="helpIcon m-p-w-250 td-wid-15" scope="col">
                                                    <div class="sortWrap">
                                                        <i class="icon ion-arrow-down-c addColor"></i>

                                                    </div>Completed%<i class="icon ion-ios-help-outline"></i>
                                                    <div class="toottipWrap"></div>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ui_tblData_complchats"></tbody>
                                    </table>
                                </div>
                            </div>
                            @{ Html.RenderPartial("_Paging"); }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script src="~/js/Chat/AgentReport.js"></script>

