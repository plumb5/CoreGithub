                                    </div>
                                </div>

                                <div class="btnWrap">
                                    <button id="ui_txtSearchFilter" type="button" class="btn-active btn-block">Search</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="scheduledresponses">
                <div class="subdivWrap border-0">
                    <div class="selectedItemWrap">
                        <div class="checkBxCount">
                            <span class="checkedCount">0</span> Contacts selected
                        </div>
                        <div class="addDropdownWrap ContributePermission">
                            <select id="ui_ddlGroup" class="form-control form-control-sm addCampName w-100"
                                    data-placeholder="Add to Group">
                                <option label="Choose one">Add to Group</option>
                            </select>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <div class="box-white border-0 position-relative">
                            <div class="tableWrapper">
                                <div class="table-responsive campaignresponse">
                                    <table id="ui_tableReport" class="table tbl-frmrepresp allvisitors">
                                        <thead>
                                            <tr>
                                                <th class="td-wid-5 m-p-w-40" scope="col">
                                                    <div class="custom-control custom-checkbox">
                                                        <input type="checkbox"
