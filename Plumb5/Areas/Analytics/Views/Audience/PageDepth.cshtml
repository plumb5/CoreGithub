@{
    ViewBag.Title = "Plumb5 :: Page Depth";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}

<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Behaviour</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Web</a></li>
            <li class="breadcrumb-item"><a href="#">Analytics Overview</a></li>
            <li class="breadcrumb-item active" aria-current="page">Behaviour</li>
        </ol>
    </div>

    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Dashboard/NewReturn">New vs Return</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Dashboard/TimeOnSite">Time on Site</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Dashboard/TimeTrends">Time Trends</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Audience/Frequency">Frequency</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Audience/Recency">Recency</a></div>
            <div class="tabItem"><a class="tap-nav-link" href="/Analytics/Audience/TimeSpend">Time Spent</a></div>
            <div class="tabItem"><a class="tap-nav-link active" href="/Analytics/Audience/PageDepth">Page Depth</a></div>
        </div>
        @{ Html.RenderPartial("_Export"); }

    </div>
    <div class="box-white mr-tp-15 ">
        @{ Html.RenderPartial("_PageHeadingWithDate"); }
        <div class="charteeWrap position-relative h-350">
            <canvas id="graphData"></canvas>
        </div>
    </div>
</section>

<section id="ui_divReportSummary" class="sheet">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white mt-4">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table id="ui_tblReportData" class="table">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-150">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Page Depth <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap"></div>
                                    </th>
                                    <th class="helpIcon m-p-w-120" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Sessions <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap"></div>
                                    </th>
                                    <th class="helpIcon m-p-w-160" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Unique Visitors <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap"></div>
                                    </th>
                                    <th class="helpIcon m-p-w-140" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Page Views <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap"></div>
                                    </th>
                                    <th class="helpIcon m-p-w-140" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>Average Time <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap"></div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_tbodyReportData"></tbody>
                        </table>
                    </div>
                </div>
                <div class="tbl-spacer"></div>
            </div>
        </div>
    </div>

</section>

<script src="~/js/Analytics/Web_PageDepth.js"></script>
