@{
    ViewBag.Title = "Plumb5 :: Paid Campaigns";
    Layout = "~/Views/Shared/PlumbAccountMaster.cshtml";
}
<section class="sheet">
    <div class="pageheader">
        <h6 class="pagetitle">Paid Campaigns</h6>
        <ol class="breadcrumb slim-breadcrumb">
            <li class="breadcrumb-item"><a href="#">Web</a></li>
            <li class="breadcrumb-item"><a href="#">Traffic Sources</a></li>
            <li class="breadcrumb-item active" aria-current="page">Paid Campaigns</li>
        </ol>
    </div>
    <div class="topTabWrap">
        <div class="iconTopNav">
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
            <div class="topIconItem"></div>
        </div>
        <div class="tabItemWrap">
            <div class="tabItem"><a class="tap-nav-link active" href="javascript:void(0)">UTM Tags</a></div>
            <div class="tabItem">
                <a class="tap-nav-link d-flex" href="/Analytics/Traffic/Adwords">
                    <i class="iconadwords"></i><span>Adwords</span>
                </a>
            </div>
            <div class="tabItem">
                <a class="tap-nav-link d-flex" href="/Analytics/Traffic/Adsense">
                    <i class="iconadsense"></i>Adsense
                </a>
            </div>
        </div>
        @{ Html.RenderPartial("_Export"); }
    </div>
    <div class="box-white mr-tp-15">
        <div class="showDateIconWrap">
            <div class="showDateWrap brd-bttm-none mb-0">
                @{ Html.RenderPartial("_PageHeadingWithDate"); }
            </div>
            <div class="iconsWrap-docu" id="ui_div_QuickFilterBy">
                <div class="lmsallsource mr-2">
                    <select name="" class="form-control form-control-sm" id="ui_drpdwn_QuickFilterBy">
                        <option value="select">Quick Filter By</option>
                        <option value="Sources">Sources</option>
                        <option value="Medium">Medium</option>
                        <option value="Campaign">Campaign</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
    <div class="subdivWrap lmsfilthite border-top-0 position-relative">
        <div class="selectedItemWrap justify-content-between">
            <div class="checkBxCount">
                <span id="ui_spn_MaxCount" class="checkedCount"></span> Result(s)
            </div>
            <div class="lmsquickfilterwrp">
                <div class="lmssourcefilterwrp hideDiv">
                    <label for="" class="frmlbltxt w-100 mb-0">Source By</label>
                    <div class="lmsdrpdwnsel">
                        <select class="form-control select2drpdwnbrd" data-live-search="true" data-placeholder="Select Sources" id="ui_drpdwn_Source"></select>
                        <i class="fa fa-close ml-2 clselmsfiltpaid"></i>
                    </div>
                </div>
                <div class="lmsmediumfilterwrp hideDiv">
                    <label for="" class="frmlbltxt w-100 mb-0">Medium</label>
                    <div class="lmsdrpdwnsel">
                        <select class="form-control select2drpdwnbrd" id="ui_drpdwn_Medium"></select>
                        <i class="fa fa-close ml-2 clselmsfiltpaid"></i>
                    </div>
                </div>
                <div class="lmscampaignfilterwrp mr-30 hideDiv">
                    <label for="" class="frmlbltxt w-100 mb-0">Campaigns</label>
                    <div class="lmsdrpdwnsel">
                        <select class="form-control select2drpdwnbrd" id="ui_drpdwn_Campaigns"></select>
                        <i class="fa fa-close ml-2 clselmsfiltpaid"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="box-white border-top-0">
                <div class="tableWrapper">
                    <div class="table-responsive">
                        <table class="table temppreveye-tbl" id="ui_tableReport">
                            <thead>
                                <tr>
                                    <th scope="col" class="helpIcon m-p-w-80 td-wid-3 text-center">Preview</th>
                                    <th scope="col" class="helpIcon m-p-w-190 td-wid-19">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Landing Pages <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-150 td-wid-12" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Page Views <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160 td-wid-11" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Sessions <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160 td-wid-14" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Unique Visitors<i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160 td-wid-10" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        Source<i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160 td-wid-10" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        Medium<i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-160 td-wid-12" scope="col">
                                        <div class="sortWrap"><i class="icon ion-arrow-down-c addColor"></i></div>
                                        Campaign<i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                    <th class="helpIcon m-p-w-190 td-wid-10" scope="col">
                                        <div class="sortWrap">
                                            <i class="icon ion-arrow-down-c addColor"></i>
                                        </div>Date <i class="icon ion-ios-help-outline"></i>
                                        <div class="toottipWrap">

                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="ui_trbodyReportData">
                            </tbody>
                        </table>
                    </div>
                </div>
                @{ Html.RenderPartial("_Paging"); }
            </div>
        </div>
    </div>
</section>
<script src="~/js/Analytics/Web_Traffic_UtmTags.js"></script>
